{
  "name": "leverage-tokens-subgraph",
  "license": "MIT",
  "scripts": {
    "generate-manifest-base": "mustache ./networks.json ./templates/8453/subgraph.template.yaml > ./subgraph.yaml",
    "generate-manifest-ethereum": "mustache ./networks.json ./templates/1/subgraph.template.yaml > ./subgraph.yaml",
    "generate-addresses-base": "mustache ./networks.json ./templates/8453/addresses.template.ts > ./src/constants/addresses.ts",
    "generate-addresses-ethereum": "mustache ./networks.json ./templates/1/addresses.template.ts > ./src/constants/addresses.ts",
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy": "graph deploy --node https://api.studio.thegraph.com/deploy/ leverage-tokens-subgraph",
    "create-local": "graph create --node http://localhost:8020/ leverage-tokens-subgraph",
    "remove-local": "graph remove --node http://localhost:8020/ leverage-tokens-subgraph",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 leverage-tokens-subgraph"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.97.0",
    "@graphprotocol/graph-ts": "0.37.0"
  },
  "packageManager": "yarn@1.22.19+sha1.4ba7fc5c6e704fce2066ecbfb0b0d8976fe62447",
  "devDependencies": {
    "mustache": "^4.2.0"
  }
}
